#include <iostream>
using namespace std;

class Polynomial {
    int coeff[3];
public:
    void input() {
        for (int i = 0; i < 3; i++)
            cin >> coeff[i];
    }

    void display() {
        cout << coeff[0] << "x^2 + "
             << coeff[1] << "x + "
             << coeff[2];
    }
};

int main() {
    Polynomial p;
    p.input();
    p.display();
    return 0;
}
