#include <iostream>
using namespace std;

int main() {
    int a = 5;

    cout << "Pre-increment: " << ++a << endl;
    cout << "Post-increment: " << a++ << endl;
    cout << "After increment: " << a << endl;

    cout << "Pre-decrement: " << --a << endl;
    cout << "Post-decrement: " << a-- << endl;
    cout << "After decrement: " << a << endl;

    return 0;
}
