#include <iostream>
using namespace std;

class Arithmetic {
public:
    int a, b;

    void input() {
        cin >> a >> b;
    }

    void calculate() {
        cout << "Sum = " << a + b << endl;
        cout << "Difference = " << a - b << endl;
        cout << "Product = " << a * b << endl;
        cout << "Quotient = " << a / b << endl;
    }
};

int main() {
    Arithmetic obj;
    obj.input();
    obj.calculate();
    return 0;
}
