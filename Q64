#include <iostream>
using namespace std;

class B;   

class A {
    int x;
public:
    A() { x = 10; }
    friend void display(A, B);
};

class B {
    int y;
public:
    B() { y = 20; }
    friend void display(A, B);
};

void display(A a, B b) {
    cout << a.x << " " << b.y;
}

int main() {
    A a;
    B b;
    display(a, b);
    return 0;
}
