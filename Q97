#include <iostream>
using namespace std;

class Demo {
    int x;
public:
    Demo(int a = 0) {
        x = a;
    }

    Demo operator+(Demo d) {
        return Demo(x + d.x);
    }

    void show() {
        cout << x;
    }
};

int main() {
    Demo a(10), b(20);
    Demo c = a + b;
    c.show();
    return 0;
}
