#include <iostream>
using namespace std;

int main() {
    int choice;
    int a=120;
    int b=20;

    cout << "Enter your choice: ";
    cin >> choice;

    switch(choice) {
        case 1:
            cout << "Addition = " << a + b;
            break;
        case 2:
            cout << "Subtraction = " << a - b;
            break;
        case 3:
            cout << "Multiplication = " << a * b;
            break;
        case 4:
            cout << "Division = " << a / b;
            break;
        default:
            cout << "Invalid choice";
    }

    return 0;
}
