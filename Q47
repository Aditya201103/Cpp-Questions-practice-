#include <iostream>
using namespace std;

class Largest {
public:
    int a, b;

    void input() {
        cin >> a >> b;
    }

    void show() {
        if (a > b)
            cout << a;
        else
            cout << b;
    }
};

int main() {
    Largest l;
    l.input();
    l.show();
    return 0;
}
