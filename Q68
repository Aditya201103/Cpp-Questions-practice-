#include <iostream>
using namespace std;

class B;

class A {
    int x;
public:
    A() { x = 10; }
    friend void compare(A, B);
};

class B {
    int y;
public:
    B() { y = 20; }
    friend void compare(A, B);
};

void compare(A a, B b) {
    if (a.x > b.y)
        cout << "A is greater";
    else
        cout << "B is greater";
}

int main() {
    A a;
    B b;
    compare(a, b);
    return 0;
}
