#include <iostream>
using namespace std;

int main() {
    int a = 10;
    int b = 3;

    // Implicit type casting
    float result1 = a / b;
    cout << "Implicit casting result (a/b): " << result1 << endl;

    // Explicit type casting
    float result2 = (float)a / b;
    cout << "Explicit casting result (a/b): " << result2 << endl;

    return 0;
}
